

module Full_SPM(
input clk, rst, start, button1, button2, input signed [7:0] multiplicand, input signed [7:0] multiplier, output done, output [6:0] display ,output [3:0] en);
    
 

 wire[15:0] prod;
    
 spm s(.clk(clk), .rst(rst), .start(start), .done(done), .multiplicand(multiplicand), .multiplier(multiplier), .product(prod));
 
 
 Binary_To_Seg b(.clk(clk), .rst(rst), .button1(button1), .button2(button2), .bin(prod), .display(display), .en(en));
 
 
 
 
 
 
 
 
endmodule
